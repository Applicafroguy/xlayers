{
  "name": "xlayers",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration hmr",
    "build": "bazel build //src:bundle",
    "postbuild": "sh scripts/tag-deploy.sh",
    "test": "ng test",
    "test:ci": "ng test --watch=false --sourceMap=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod && firebase deploy",
    "####": "##### BAZEL SCRIPTS ####",
    "postinstall": "ngc -p tsconfig.post.json",
    "bazel:build": "bazel build //src:bundle",
    "ibazel:build": "ibazel build //src:bundle",
    "postbazel:build": "sh scripts/tag-deploy.sh",
    "bazel:start": "ibazel run //src:devserver",
    "bazel:start:prod": "bazel run //src:prodserver",
    "bazel:e2e": "bazel test //e2e:all",
    "bazel:test": "bazel test //src/...",
    "prebazel:buildifier": "bazel build @com_github_bazelbuild_buildtools//buildifier",
    "bazel:buildifier": "find . -type f \\( -name BUILD -or -name BUILD.bazel \\) ! -path \"./node_modules/*\" | xargs $(bazel info bazel-bin)/external/com_github_bazelbuild_buildtools/buildifier/*/buildifier"
  },
  "private": true,
  "dependencies": {
    "@ngxs/store": "^3.0.1",
    "angular-resizable-element": "^3.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jszip": "^3.1.5",
    "ngx-color": "^1.4.0",
    "ngx-monaco-editor": "^6.0.0",
    "rxjs": "^6.0.0",
    "source-map-support": "^0.5.9",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-ng-packagr": "~0.6.3",
    "@angular/animations": "^6.1.1",
    "@angular/bazel": "^6.1.7",
    "@angular/cdk": "^6.4.2",
    "@angular/cli": "~6.0.3",
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/http": "^6.1.1",
    "@angular/language-service": "^6.1.1",
    "@angular/material": "^6.1.1",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/pwa": "^0.7.5",
    "@angular/router": "^6.1.1",
    "@angular/service-worker": "^6.1.1",
    "@angularclass/hmr": "^2.1.3",
    "@bazel/benchmark-runner": "0.1.0",
    "@bazel/ibazel": "^0.5.0",
    "@bazel/typescript": "0.17.0",
    "@ngxs/devtools-plugin": "^3.0.1",
    "@ngxs/logger-plugin": "^3.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "big-integer": "^1.6.30",
    "buffer": "^5.1.0",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "^5.4.0",
    "puppeteer": "^1.8.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "2.9.2"
  }
}
